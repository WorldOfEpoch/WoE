        <!-- Social Member Entry Prefab Placeholder -->
        <ui:VisualElement name="SocialMemberEntryPlaceholder" />

        <!-- Buttons -->
        <ui:Button name="WhisperButton" text="Whisper" clickEvent="WhisperOptionClicked" />
