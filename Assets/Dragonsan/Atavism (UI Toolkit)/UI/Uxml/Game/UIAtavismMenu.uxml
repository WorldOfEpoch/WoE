        <ui:Button name="PrivateInstanceButton" text="Private Instance" clickEvent="InvitePrivateInstance" />

        <!-- Member Popup Placeholder -->
        <ui:VisualElement name="MemberPopupPlaceholder" />

        <!-- ... (Other UI elements based on the MonoBehaviour) -->
    </ui:VisualElement>
