.character-stat-row {
    flex-direction: row;
    margin-left: 2px;
    margin-right: 2px;
    margin-top: 2px;
    margin-bottom: 2px;
    justify-content: space-between;
    align-self: stretch;
    align-items: center;
    flex-grow: 1;
    width: 190px;
}

.character-stat-label {
    margin-left: 0;
    margin-right: 5px;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    -unity-text-align: middle-right;
    flex-grow: 1;
}

.character-stat-value {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    width: 50px;
    -unity-text-align: middle-left;
    flex-shrink: 0;
}

.equip-set-selected {
    color: var(--color-atavismTextLight);
    border-left-color: rgb(223, 205, 177);
    border-right-color: rgb(223, 205, 177);
    border-top-color: rgb(223, 205, 177);
    border-bottom-color: rgb(223, 205, 177);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.equip-set-button {
    -unity-font-style: bold;
    font-size: 14px;
    -unity-text-align: middle-center;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
}

.character-panel-sets-button {
    background-color: rgba(48, 48, 48, 0.34);
}

.character-panel-sets-button:hover:enabled {
    background-color: rgba(48, 48, 48, 0.89);
}

.character-stat-button {
    background-color: rgba(42, 40, 39, 0);
    -unity-background-image-tint-color: rgb(200, 169, 141);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    width: 17px;
    height: 34px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.character-stat-button:hover:enabled {
    background-color: rgba(42, 40, 39, 0);
    -unity-background-image-tint-color: rgba(200, 169, 141, 0.73);
}

.equip-slot-container {
    width: 45px;
    height: 45px;
    position: relative;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    align-items: stretch;
    justify-content: flex-start;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    max-height: 45px;
    min-height: 45px;
    min-width: 45px;
    max-width: 45px;
    align-self: stretch;
}

.equip-slot-icon {
    background-image: url("project://database/Assets/Dragonsan/AtavismObjects/Textures/NewUi/Inventory/invenory_slot_v2.png?fileID=2800000&guid=f480ff160d7214e4b82bf113864e5569&type=3#invenory_slot_v2");
    width: 45px;
    height: 45px;
    position: relative;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    align-items: center;
    justify-content: center;
    max-height: 45px;
    min-height: 45px;
    min-width: 45px;
    max-width: 45px;
}
